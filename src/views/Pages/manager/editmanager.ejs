<%- include('../../Partials/sidebar') %>
    <!-- ===== Content Area Start ===== -->
    <div class="relative flex flex-1 flex-col overflow-y-auto overflow-x-hidden">
        <!-- ===== Header Start ===== -->
        <%- include('../../Partials/Header') %>


            <!-- ===== Header End ===== -->

            <!-- ===== Main Content Start ===== -->
            <main>
                <div class="mx-auto max-w-screen-2xl p-4 md:p-6 2xl:p-10">
                    <!-- Breadcrumb Start -->
                    <div class="mb-6 flex flex-col gap-3 sm:flex-row sm:items-center sm:justify-between">
                        <h2 class="text-title-md2 font-bold text-black dark:text-white">
                            Update Manager
                        </h2>

                        <nav>
                            <ol class="flex items-center gap-2">
                                <li>
                                    <a class="font-medium" href="/">Dashboard /</a>
                                </li>
                                <li class="font-medium text-primary">Update Manager</li>
                            </ol>
                        </nav>
                    </div>
                    <!-- Breadcrumb End -->

                    <!-- ====== Update Product Section Start -->
                    <div class="grid grid-cols-1 gap-9 sm:grid-cols-2">
                        <div class="flex flex-col gap-9">
                            <!-- Contact Form -->
                            <div
                                class="rounded-sm border border-stroke bg-white shadow-default dark:border-strokedark dark:bg-boxdark">
                                <div class="border-b border-stroke px-6.5 py-4 dark:border-strokedark">
                                    <h3 class="font-medium text-black dark:text-white">
                                        Update Manager Details
                                    </h3>
                                </div>
                                <form action="" method="post">
                                    <div class="p-6.5">
                                        <div class="mb-4.5 flex flex-col gap-6 xl:flex-row">
                                            <div class="w-full ">
                                                <label
                                                    class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                    Manager's Name
                                                </label>
                                                <input type="text" name="name" value="<%= manager.name %>" placeholder="i.e, Virat Kohli"
                                                    class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-3 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                    required />
                                            </div>
                                        </div>
                                        <div class="mb-4.5 flex flex-col gap-6 xl:flex-row">
                                            <div class="w-full ">
                                                <label
                                                    class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                    Manager's Email
                                                </label>
                                                <input type="email" name="email" value="<%= manager.email %>" placeholder="i.e, virat@gmail.com"
                                                    class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-3 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                    required />
                                            </div>
                                        </div>
                                        <div class="mb-4.5 flex flex-col gap-6 xl:flex-row">
                                            <div class="w-full ">
                                                <label
                                                    class="mb-3 block text-sm font-medium text-black dark:text-white">
                                                    Manager's Password
                                                </label>
                                                <input type="text" name="password" value="<%= manager.show %>" placeholder="i.e, ###"
                                                    class="w-full rounded border-[1.5px] border-stroke bg-transparent px-5 py-3 font-normal text-black outline-none transition focus:border-primary active:border-primary disabled:cursor-default disabled:bg-whiter dark:border-form-strokedark dark:bg-form-input dark:text-white dark:focus:border-primary"
                                                    required />
                                            </div>
                                        </div>
                                        <button
                                            class="flex w-full justify-center rounded bg-primary p-3 font-medium text-gray hover:bg-opacity-90">
                                            Update Manager
                                        </button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <!-- ====== Update Product Section End -->
                </div>
            </main>
            <!-- ===== Main Content End ===== -->
    </div>
    <!-- ===== Content Area End ===== -->
    </div>
    <!-- ===== Page Wrapper End ===== -->
    <script defer src="/bundle.js"></script>
    </body>

    </html>